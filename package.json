{
  "name": "iter-tools",
  "version": "4.0.2",
  "description": "The iterator toolbox",
  "main": "es5/index",
  "module": "es5/index.mjs",
  "jsnext:main": "es5/index.mjs",
  "typings": "src/index.d.ts",
  "scripts": {
    "lint": "eslint --fix --ext .mjs ./src ./test ./benchmarks",
    "test": "npm run clean && npm run build:es2018-cjs && npm run build:es5-cjs && mocha",
    "watch": "npm run test -- -w",
    "benchmarks": "npm run clean && npm run build:es2018-cjs && node benchmarks",
    "clean": "rimraf es5 es2015 es2018",
    "build": "npm run clean && npm run build:es5 && npm run build:es5-cjs && npm run build:es2015 && npm run build:es2015-cjs && npm run build:es2018 && npm run build:es2018-cjs",
    "build:es5": "cross-env BABEL_ENV=es5 babel --keep-file-extension -x \".mjs\" src -d es5",
    "build:es5-cjs": "cross-env BABEL_ENV=es5-cjs babel -x \".mjs\" src -d es5",
    "build:es2015": "cross-env BABEL_ENV=es2015 babel --keep-file-extension -x \".mjs\" src -d es2015",
    "build:es2015-cjs": "cross-env BABEL_ENV=es2015-cjs babel -x \".mjs\" src -d es2015",
    "build:es2018": "mkdirp es2018 && copyfiles -f src/*.mjs es2018",
    "build:es2018-cjs": "cross-env BABEL_ENV=cjs babel -x \".mjs\" src -d es2018",
    "prepublish": "npm run build",
    "precommit": "npm run lint",
    "prepush": "npm run test"
  },
  "keywords": [
    "iterators",
    "itertools",
    "generators",
    "async"
  ],
  "repository": "git@github.com:sithmel/iter-tools.git",
  "author": "Maurizio Lupo <maurizio.lupo@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.49",
    "clone-regexp": "^1.0.1",
    "dequeue": "^1.0.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.49",
    "@babel/core": "^7.0.0-beta.49",
    "@babel/plugin-proposal-async-generator-functions": "^7.0.0-beta.49",
    "@babel/plugin-syntax-async-generators": "^7.0.0-beta.49",
    "@babel/plugin-transform-modules-commonjs": "^7.0.0-beta.49",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.49",
    "@babel/preset-env": "^7.0.0-beta.49",
    "babel-eslint": "^8.2.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "chai": "^4.1.2",
    "copyfiles": "^2.0.0",
    "cross-env": "^5.1.6",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "husky": "^0.14.3",
    "json": "^9.0.6",
    "measure-speed": "^1.0.0",
    "mkdirp": "^0.5.1",
    "mocha": "^5.0.5",
    "require-all": "^2.2.0",
    "rimraf": "^2.6.2"
  }
}
